msgid "ScrollBarCSS"
msgstr ""
    "QScrollBar:vertical{"
    "border: none;"
    "background: #222;"
    "width: 16px;"
    "margin: 16px 0px 16px 0px;"
    "}"
    "QScrollBar::handle:vertical{"
    "background: #555;"
    "min-height: 32px;"
    "border-radius: 5px;"
    "}"
    "QScrollBar::handle:vertical:hover{"
    "background: #777;"
    "}"
    "/* Pfeile vertikal */"
    "QScrollBar::sub-line:vertical{"
    "background: #333;"
    "height: 16px;"
    "subcontrol-position: top;"
    "subcontrol-origin: margin;"
    "font-family: \"Arial\";"
    "font-size: 10px;"
    "color:white;"
    "}"
    "QScrollBar::sub-line:vertical::after {"
    "content: \"\\25B2\";"
    "}"
    "QScrollBar::add-line:vertical {"
    "background: #333;"
    "height: 16px;"
    "subcontrol-position: bottom;"
    "font-family: \"Arial\";"
    "font-size: 10px;"
    "color: white;"
    "}"
    "QScrollBar::add-line:vertical::after {"
    "content: \"\\25BC\"; /* Pfeil nach unten */"
    "}"
    "QScrollBar:horizontal {"
    "border: none;"
    "background: #222;"
    "height: 16px;"
    "margin: 0 16px 0 16px;"
    "}"
    "QScrollBar::handle:horizontal {"
    "background: #555;"
    "min-width: 32px;"
    "border-radius: 5px;"
    "}"
    "QScrollBar::handle:horizontal:hover {"
    "background: #777;"
    "}"
    "QScrollBar::sub-line:horizontal {"
    "background: #333;"
    "width: 16px;"
    "subcontrol-position: left;"
    "font-family: \"Arial\";"
    "font-size: 10px;"
    "color: white;"
    "}"
    "QScrollBar::sub-line:horizontal::after {"
    "content: \"\\25C0\";"
    "}"
    "QScrollBar::add-line:horizontal {"
    "background: #333;"
    "width: 16px;"
    "subcontrol-position: right;"
    "font-family: \"Arial\";"
    "font-size: 10px;"
    "color: white;"
    "}"
    "QScrollBar::add-line:horizontal::after {"
    "content: \"\\25B6\";"
    "}"
    "QScrollBar::sub-line:vertical:hover,"
    "QScrollBar::add-line:vertical:hover,"
    "QScrollBar::sub-line:horizontal:hover,"
    "QScrollBar::add-line:horizontal:hover { background: #cc0;"
    "}"
    "QScrollBar::sub-line:vertical:pressed,"
    "QScrollBar::add-line:vertical:pressed,"
    "QScrollBar::sub-line:horizontal:pressed,"
    "QScrollBar::add-line:horizontal:pressed { background: green;}"
    "/* Seitenbereich transparent */"
    "QScrollBar::add-page:vertical,"
    "QScrollBar::sub-page:vertical,"
    "QScrollBar::add-page:horizontal,"
    "QScrollBar::sub-page:horizontal { background: none; }"

